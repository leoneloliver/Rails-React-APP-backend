<div id="<%= dom_id recipe %>">
  <p>
    <strong>Title:</strong>
    <%= recipe.title %>
  </p>

  <p>
	<img src="<%= recipe.photo %>" class="img"/>
  </p>

  <p>
    <strong>Ingredientes:</strong>
    <%= recipe.ingredientes %>
  </p>

  <p>
    <strong>Preparation:</strong>
    <%= recipe.preparation %>
  </p>

</div>
